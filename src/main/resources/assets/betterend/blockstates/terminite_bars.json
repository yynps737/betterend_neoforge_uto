{
  "multipart": [
    {
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      },
      "apply": {
        "model": "betterend:block/terminite_bars_post",
        "uvlock": true
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "betterend:block/terminite_bars_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "betterend:block/terminite_bars_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "betterend:block/terminite_bars_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "betterend:block/terminite_bars_side",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
